-- Function to create a notification message
function createNotification(title, text, callback)
    local ScreenGui = Instance.new("ScreenGui")
    local Frame = Instance.new("Frame")
    local TextLabel = Instance.new("TextLabel")
    local YesButton = Instance.new("TextButton")
    local NoButton = Instance.new("TextButton")

    ScreenGui.Parent = game.CoreGui

    Frame.Parent = ScreenGui
    Frame.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
    Frame.Position = UDim2.new(0.5, -100, 0.5, -50)
    Frame.Size = UDim2.new(0, 200, 0, 100)
    Frame.Active = true
    Frame.Draggable = true

    TextLabel.Parent = Frame
    TextLabel.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
    TextLabel.BackgroundTransparency = 1
    TextLabel.Position = UDim2.new(0, 0, 0, 10)
    TextLabel.Size = UDim2.new(1, 0, 0, 40)
    TextLabel.Font = Enum.Font.SourceSans
    TextLabel.Text = text
    TextLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
    TextLabel.TextScaled = true

    YesButton.Parent = Frame
    YesButton.BackgroundColor3 = Color3.fromRGB(0, 255, 0)
    YesButton.Position = UDim2.new(0.1, 0, 0.6, 0)
    YesButton.Size = UDim2.new(0.35, 0, 0.3, 0)
    YesButton.Font = Enum.Font.SourceSans
    YesButton.Text = "Yes"
    YesButton.TextColor3 = Color3.fromRGB(0, 0, 0)
    YesButton.TextScaled = true

    NoButton.Parent = Frame
    NoButton.BackgroundColor3 = Color3.fromRGB(255, 0, 0)
    NoButton.Position = UDim2.new(0.55, 0, 0.6, 0)
    NoButton.Size = UDim2.new(0.35, 0, 0.3, 0)
    NoButton.Font = Enum.Font.SourceSans
    NoButton.Text = "No"
    NoButton.TextColor3 = Color3.fromRGB(0, 0, 0)
    NoButton.TextScaled = true

    YesButton.MouseButton1Click:Connect(function()
        callback(true)
        ScreenGui:Destroy()
    end)

    NoButton.MouseButton1Click:Connect(function()
        callback(false)
        ScreenGui:Destroy()
    end)
end

-- Example usage of the notification
createNotification("Notification", "Would you like to load Free Emotes?", function(answer)
    if answer then
        loadstring(game:HttpGet("https://rawscripts.net/raw/Universal-Script-AFEM-14048"))()
    end
end)

