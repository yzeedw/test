-- Define the global variables
getgenv().Sky = false  -- Initialize Sky as false
getgenv().SkyAmount = 1600

-- Function to handle the toggle state and notifications
function toggleSky(state)
    getgenv().Sky = state
    if state then
        -- Show the "AntiLock On" notification
        game.StarterGui:SetCore("SendNotification", {
            Title = "AntiLock";
            Text = "AntiLock On";
            Duration = 4;
        })
    else
        -- Show the "AntiLock Off" notification
        game.StarterGui:SetCore("SendNotification", {
            Title = "AntiLock";
            Text = "AntiLock Off";
            Duration = 4;
        })
    end
end

-- Connect the heartbeat event to the script's logic
game:GetService("RunService").Heartbeat:Connect(function()
    if getgenv().Sky then 
        local player = game.Players.LocalPlayer
        local character = player.Character
        if character and character:FindFirstChild("HumanoidRootPart") then
            local vel = character.HumanoidRootPart.Velocity
            character.HumanoidRootPart.Velocity = Vector3.new(0, getgenv().SkyAmount, 0)
            game:GetService("RunService").RenderStepped:Wait()
            character.HumanoidRootPart.Velocity = vel
        end
    end
end)

-- Add the Section:Toggle element to your existing GUI library
Section:Toggle({
    Text = "AntiLock",
    Callback = function(state)
        toggleSky(state)  -- Call the toggleSky function with the new state
        -- Update the button text based on the state
        if state then
            ToggleButton.Text = "AntiLock ON"
        else
            ToggleButton.Text = "AntiLock OFF"
        end
    end
})


