-- Define the global variables
getgenv().ResolverEnabled = false  -- Initialize ResolverEnabled as false
local Players = game:GetService("Players")
local resolverConnection  -- Variable to hold the PlayerAdded connection

-- Function to handle the toggle state and notifications
function toggleResolver(state)
    getgenv().ResolverEnabled = state
    
    if state then
        -- Show the "Resolver On" notification
        game.StarterGui:SetCore("SendNotification", {
            Title = "Resolver";
            Text = "Resolver On";
            Duration = 4;
        })

        -- Function to calculate velocity
        local function calculateVelocity(humanoid)
            local lastPosition = humanoid.RootPart.Position
            local lastTime = tick()

            return function()
                local currentPosition = humanoid.RootPart.Position
                local currentTime = tick()
                local deltaTime = currentTime - lastTime

                local predictedVelocity = (currentPosition - lastPosition) / deltaTime

                lastPosition = currentPosition
                lastTime = currentTime

                return predictedVelocity
            end
        end

        -- Function to update humanoid velocity
        local function updateHumanoidVelocity(player)
            local humanoid = player.Character and player.Character:FindFirstChildOfClass("Humanoid")
            if humanoid then
                local velocityFunc = calculateVelocity(humanoid)

                humanoid:GetPropertyChangedSignal("RootPart"):Connect(function()
                    if getgenv().ResolverEnabled then
                        local velocity = velocityFunc()

                        humanoid.WalkSpeed = velocity.magnitude * 0.132
                        humanoid.MoveDirection = velocity.unit
                    end
                end)
            end
        end

        -- Update existing players
        for _, player in ipairs(Players:GetPlayers()) do
            updateHumanoidVelocity(player)
        end

        -- Connect to PlayerAdded
        resolverConnection = Players.PlayerAdded:Connect(updateHumanoidVelocity)
    else
        -- Show the "Resolver Off" notification
        game.StarterGui:SetCore("SendNotification", {
            Title = "Resolver";
            Text = "Resolver Off";
            Duration = 4;
        })
        
        -- Stop updating humanoid velocity
        if resolverConnection then
            resolverConnection:Disconnect()
            resolverConnection = nil  -- Clear the connection variable
        end
    end
end

-- Add the Section:Toggle element to your existing GUI library
Section:Toggle({
    Text = "Resolver",
    Callback = function(state)
        toggleResolver(state)  -- Call the toggleResolver function with the new state
    end
})

